{
  "name": "productshare-web3-commerce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev": "concurrently npm:dev:*",
    "build": "tsc",
    "start:server": "node ./dist/index.js",
    "populate": "node ./dist/populate.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently npm:start:*",
    "migration:generate": "ts-node migration generate",
    "migration:run": "ts-node migration run",
    "migration:revert": "ts-node migration revert",
    "stripe": "stripe listen --forward-to localhost:3000/payments/stripe"
  },
  "dependencies": {
    "@apollo/client": "^3.12.5",
    "@aws-sdk/client-s3": "^3.726.1",
    "@aws-sdk/credential-providers": "^3.726.1",
    "@aws-sdk/lib-storage": "^3.726.1",
    "@pinelab/vendure-plugin-coinbase": "^1.3.0",
    "@privy-io/server-auth": "^1.17.2",
    "@vendure/admin-ui-plugin": "2.3.0",
    "@vendure/asset-server-plugin": "2.3.0",
    "@vendure/core": "2.3.0",
    "@vendure/create": "^2.3.0",
    "@vendure/email-plugin": "2.3.0",
    "@vendure/payments-plugin": "^2.3.0",
    "@vendure/ui-devkit": "^2.3.0",
    "dotenv": "16.4.7",
    "pg": "8.13.1",
    "stripe": "^13.11.0",
    "typescript": "5.7.3"
  },
  "devDependencies": {
    "@vendure/cli": "^2.3.0",
    "concurrently": "9.1.2",
    "prettier": "^3.4.2",
    "ts-node": "10.9.2"
  }
}
